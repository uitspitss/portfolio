{
  "engines": {
    "node": "8"
  },
  "scripts": {
    "dev": "next \"src/app/\"",
    "preserve": "npm run build-public && npm run build-funcs && npm run build-app && npm run copy-deps && npm run install-deps",
    "serve": "cross-env NODE_ENV=production firebase serve",
    "predeploy": "npm run build-public && npm run build-funcs && npm run build-app && npm run copy-deps",
    "deploy": "firebase deploy",
    "clean": "rimraf \"dist/functions/**\" && rimraf \"dist/public\"",
    "build-public": "cpx \"src/public/**/*.*\" \"dist/public\" -C",
    "build-funcs": "babel \"src/functions\" --out-dir \"dist/functions\" && cpx \"src/functions/.runtimeconfig.json\" \"dist/functions\" -C",
    "build-app": "next build \"src/app/\"",
    "postbuild-app": "cpx \"src/app/static/*\" \"dist/functions/static\" -C",
    "copy-deps": "cpx \"*{package.json,package-lock.json,yarn.lock}\" \"dist/functions\" -C",
    "install-deps": "cd \"dist/functions\" && npm i",
    "storybook": "start-storybook -s ./src/app -p 6006"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.15",
    "@fortawesome/free-brands-svg-icons": "^5.7.2",
    "@fortawesome/free-solid-svg-icons": "^5.7.2",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "@material-ui/core": "^4.1.1",
    "@material-ui/icons": "^4.0.1",
    "@material-ui/styles": "^4.1.1",
    "@zeit/next-mdx": "^1.2.0",
    "axios": "^0.19.0",
    "firebase-admin": "^8.1.0",
    "firebase-functions": "^3.0.1",
    "formik": "^1.5.7",
    "jss": "^9.8.7",
    "next": "^8.0.3",
    "next-compose-plugins": "^2.2.0",
    "nodemailer": "^6.1.1",
    "nodemailer-sendgrid-transport": "^0.2.0",
    "prop-types": "^15.7.2",
    "react": "^16.9.0-alpha.0",
    "react-dom": "^16.9.0-alpha.0",
    "react-jss": "^8.6.1",
    "react-router": "^5.0.0",
    "remark-emoji": "^2.0.2",
    "remark-images": "^0.18.0",
    "yup": "^0.27.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/core": "^7.3.4",
    "@storybook/addon-a11y": "^5.0.11",
    "@storybook/addon-actions": "^5.0.11",
    "@storybook/addon-info": "^5.0.11",
    "@storybook/addon-knobs": "^5.0.11",
    "@storybook/addon-links": "^5.0.11",
    "@storybook/cli": "^5.0.11",
    "@storybook/react": "^5.0.11",
    "@types/next": "^8.0.1",
    "@types/react": "^16.8.8",
    "@types/storybook__react": "^4.0.1",
    "@typescript-eslint/eslint-plugin": "^1.4.2",
    "@typescript-eslint/parser": "^1.4.2",
    "@zeit/next-typescript": "^1.1.1",
    "babel-loader": "^8.0.5",
    "cpx": "^1.5.0",
    "cross-env": "^5.2.0",
    "dotenv": "^8.0.0",
    "eslint": "^5.15.2",
    "eslint-config-prettier": "^5.0.0",
    "eslint-plugin-prettier": "^3.0.1",
    "firebase-functions-test": "^0.1.6",
    "firebase-tools": "^7.0.0",
    "prettier": "^1.16.4",
    "react-docgen-typescript-loader": "^3.1.0",
    "react-docgen-typescript-webpack-plugin": "^1.1.0",
    "rimraf": "^2.6.0",
    "typescript": "^3.3.3333",
    "yarn": "^1.16.0"
  }
}
