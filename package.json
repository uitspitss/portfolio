{
  "engines": {
    "node": "10"
  },
  "scripts": {
    "dev": "next \"src/app/\"",
    "preserve": "npm run build-public && npm run build-funcs && npm run build-app && npm run copy-deps && npm run install-deps",
    "serve": "cross-env NODE_ENV=production firebase emulators:start --only functions,hosting",
    "predeploy": "npm run build-public && npm run build-funcs && npm run build-app && npm run copy-deps",
    "deploy": "firebase deploy",
    "clean": "rimraf \"dist/functions/**\" && rimraf \"dist/public\"",
    "build-public": "cpx \"src/public/**/*.*\" \"dist/public\" -C && cpx \"src/app/public/**/*.*\" \"dist/public\" -C",
    "build-funcs": "tsc --project \"src/functions\" && cpx \"src/functions/.runtimeconfig.json\" \"dist/functions\" -C",
    "build-app": "NODE_ENV=production next build \"src/app/\"",
    "postbuild-app": "cpx \"src/app/*\" \"dist/functions/static\" -C",
    "copy-deps": "cpx \"*{package.json,package-lock.json,yarn.lock}\" \"dist/functions\" -C",
    "install-deps": "cd \"dist/functions\" && npm i",
    "storybook": "start-storybook -s ./src/app -p 6006",
    "use-staging": "firebase use staging",
    "use-production": "firebase use production"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.29",
    "@fortawesome/free-brands-svg-icons": "^5.13.1",
    "@fortawesome/free-solid-svg-icons": "^5.13.1",
    "@fortawesome/react-fontawesome": "^0.1.11",
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/styles": "^4.10.0",
    "@mdx-js/loader": "^1.6.6",
    "@next/mdx": "^9.4.4",
    "@zeit/next-mdx": "^1.2.0",
    "axios": "^0.19.2",
    "firebase-admin": "^8.13.0",
    "firebase-functions": "^3.7.0",
    "formik": "2.1.4",
    "imagemin-mozjpeg": "^9.0.0",
    "imagemin-optipng": "^8.0.0",
    "imagemin-svgo": "^8.0.0",
    "next": "^9.4.4",
    "next-compose-plugins": "^2.2.0",
    "next-optimized-images": "^2.6.1",
    "next-redux-saga": "^4.1.2",
    "next-redux-wrapper": "^6.0.2",
    "nodemailer": "^6.4.10",
    "nodemailer-sendgrid-transport": "^0.2.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.2.0",
    "react-vfx": "^0.2.2",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-saga": "^1.1.3",
    "remark-emoji": "^2.0.2",
    "remark-images": "^2.0.0",
    "yup": "^0.29.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.4",
    "@babel/core": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@storybook/addon-a11y": "^5.3.19",
    "@storybook/addon-actions": "^5.3.19",
    "@storybook/addon-info": "^5.3.19",
    "@storybook/addon-knobs": "^5.3.19",
    "@storybook/addon-links": "^5.3.19",
    "@storybook/cli": "^5.3.19",
    "@storybook/react": "^5.3.19",
    "@types/babel__core": "^7.1.9",
    "@types/babel__preset-env": "^7.9.0",
    "@types/cpx": "^1.5.1",
    "@types/eslint": "^7.2.0",
    "@types/eslint-plugin-prettier": "^3.1.0",
    "@types/imagemin-mozjpeg": "^8.0.0",
    "@types/imagemin-optipng": "^5.2.0",
    "@types/imagemin-svgo": "^7.0.0",
    "@types/next-redux-saga": "^3.0.2",
    "@types/nodemailer": "^6.4.0",
    "@types/parcel-bundler": "^1.12.1",
    "@types/prettier": "^2.0.2",
    "@types/react": "^16.9.41",
    "@types/react-dom": "^16.9.8",
