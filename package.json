    "@zeit/next-mdx": "^1.2.0",
    "axios": "^0.19.2",
    "firebase-admin": "^8.9.2",
    "firebase-functions": "^3.3.0",
    "formik": "2.1.4",
    "imagemin-mozjpeg": "^8.0.0",
    "imagemin-optipng": "^7.1.0",
    "imagemin-svgo": "^7.1.0",
    "next": "^9.2.2",
    "next-compose-plugins": "^2.2.0",
    "next-optimized-images": "^2.5.5",
    "next-redux-saga": "^4.1.2",
    "next-redux-wrapper": "^5.0.0",
    "nodemailer": "^6.4.3",
