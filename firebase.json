{
  "hosting": {
    "public": "src/app/out",
    "rewrites": [
      {
        "source": "/",
        "destination": "/index.html"
      },
      {
        "source": "/about",
        "destination": "/about.html"
      },
      {
        "source": "/contact",
        "destination": "/contact.html"
      },
      {
        "source": "/works",
        "destination": "/works.html"
      },
      {
        "source": "/github",
        "destination": "/github.html"
      }
    ]
  },
  "functions": {
    "source": "dist/functions"
  }
}
