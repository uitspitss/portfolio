{
  "hosting": {
    "public": "src/app/out",
    "rewrites": [
      {
        "source": "/",
        "destination": "/index.html"
      },
      {
        "source": "/about",
        "destination": "/about.html"
      },
      {
        "source": "/contact",
        "destination": "/contact.html"
      },
      {
        "source": "/works",
        "destination": "/works.html"
      },
      {
        "source": "/hobby-works",
        "destination": "/hobby-works.html"
      },
      {
        "source": "/github",
        "destination": "/github.html"
      }),
      {
        "source": "/api/**",
        "function": "next"
      }
    ]
  },
  "functions": {
    "source": "dist/functions"
  }
}
